<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="net.osgiliath.raw.parent"
	xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<fileSets>
		<fileSet filtered="true" encoding="UTF-8">
			<directory></directory>
			<includes>
				<include>README.txt</include>
				<include>readme.md</include>
				<include>.gitignore</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" encoding="UTF-8">
			<directory></directory>
			<includes>
				<include>.travis.yml</include>
				<include>build.properties</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" encoding="UTF-8">
			<directory>src/site</directory>
			<includes>
				<include>site.xml</include>
			</includes>
		</fileSet>
	</fileSets>
	<modules>
		<module id="${rootArtifactId}.features" dir="__rootArtifactId__.features" name="${rootArtifactId}.features">
			<fileSets>
				<fileSet filtered="true" encoding="UTF-8">
					<directory>src/main/resources</directory>
					<includes>
						<include>**/*.xml</include>
					</includes>
				</fileSet>
				<fileSet encoding="UTF-8">
					<directory>src/main/resources</directory>
					<includes>
						<include>**/*.cfg</include>
					</includes>
				</fileSet>
				<fileSet filtered="true" encoding="UTF-8">
					<directory></directory>
					<includes>
						<include>LICENSE.txt</include>
						<include>NOTICE.txt</include>
					</includes>
				</fileSet>
				<fileSet encoding="UTF-8">
					<directory></directory>
					<includes>
						<include>.gitignore</include>
						<include>osgi.bnd</include>
					</includes>
				</fileSet>
			</fileSets>
		</module>
	</modules>
</archetype-descriptor>
